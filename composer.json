{
    "name": "pw/laragammu",
    "description": "Gammu Notifications Driver",
    "homepage": "https://github.com/laravel-notification-channels/gammu",
    "license": "MIT",
    "authors": [{
            "name": "Muhammad Zamroni",
            "email": "halo@matriphe.com",
            "homepage": "https://matriphe.com",
            "role": "Developer"
        },
        {
            "name": "Kristian Drucker",
            "email": "kristian@rolmi.sk",
            "homepage": "https://github.com/kristiandrucker",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "predis/predis": "^1.1",
        "illuminate/database": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x",
        "illuminate/notifications": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.5",
        "orchestra/testbench": "^3.1|^3.2|^3.3",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Pw\\Gammu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pw\\Gammu\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pw\\Gammu\\GammuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
